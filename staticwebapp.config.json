{
    "navigationFallback": {
      "rewrite": "/index.html"
    },
    "responseOverrides": {
          "404": {
            "rewrite": "/404.html"
          }
      },
      "routes": [
        {
          "route": "/*",
          "serve": "/index.html",
          "statusCode": 200
        }
      ]
    }